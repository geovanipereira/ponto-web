<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="1.2"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <navigation-rule>
        <navigation-case>
            <from-outcome>admUsers</from-outcome>
            <to-view-id>/admin/users.jsf</to-view-id>
            <redirect/>
        </navigation-case>

        <navigation-case>
            <from-outcome>admRoles</from-outcome>
            <to-view-id>/admin/roles.jsf</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
</faces-config>